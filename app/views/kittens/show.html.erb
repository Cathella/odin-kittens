<h1>Show kitten</h1>
<%= render 'layouts/notice' %>

<%= link_to "Back to kittens", kittens_path %>
<div class="">
  <p><strong>Name:</strong> <%= @kitten.name %></p>
  <p><strong>Age:</strong> <%= @kitten.age %></p>
  <p><strong>Cuteness:</strong> <%= @kitten.cuteness %></p>
  <p><strong>Softness:</strong> <%= @kitten.softness %></p>
  <p><%= link_to "Remove this Kitten", @kitten, method: :delete %></p>
</div>
<%= link_to "Edit kitten", edit_kitten_path %>